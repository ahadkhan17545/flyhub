APP_NAME=FlyHub
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL="http://localhost"

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=flyhub
DB_USERNAME=root
DB_PASSWORD=

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Application Settings
REDIRECT_HTTPS=false
LOG_CHANNEL=stack
BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=database
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Laravel Passport
PASSPORT_PERSONAL_ACCESS_CLIENT_ID=1
PASSPORT_PERSONAL_ACCESS_CLIENT_SECRET=

# AWS Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_URL=
AWS_CDN_DEFAULT_REGION=us-east-1
AWS_CDN_BUCKET=
AWS_CDN_URL=

# MercadoLivre Configuration
MELI_CLIENT_ID=
MELI_CLIENT_SECRET_KEY=
MELI_ENVIRONMENT=test

# Application Configuration
SWAGGER_VERSION=2.0
BUGSNAG_API_KEY=

# Cashier Configuration
CASHIER_MODEL=App\Models\Tenant
CASHIER_CURRENCY=brl
CASHIER_CURRENCY_LOCALE=pt_BR

# Stripe Configuration
STRIPE_KEY=
STRIPE_SECRET=

# Pusher Configuration (for real-time features)
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1
MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME="${APP_NAME}"

# Queue Configuration
QUEUE_CONNECTION=database
QUEUE_FAILED_DRIVER=database-uuids

# Cache Configuration
CACHE_DRIVER=file
CACHE_PREFIX=flyhub_cache

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=false

# File Upload Configuration
FILESYSTEM_DRIVER=local
FILESYSTEM_DRIVER_PUBLIC=public

# Security Configuration
SANCTUM_STATEFUL_DOMAINS=
SESSION_DOMAIN=
